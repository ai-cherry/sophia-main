# Sophia AI Integration Environment Variables Example
# Copy this file to .env and fill in your actual values
# WARNING: Never commit .env files to version control

# Snowflake Configuration
SNOWFLAKE_ACCOUNT=your-account-identifier
SNOWFLAKE_USER=your-username
SNOWFLAKE_PASSWORD=your-password
SNOWFLAKE_WAREHOUSE=your-warehouse
SNOWFLAKE_DATABASE=your-database
SNOWFLAKE_SCHEMA=your-schema
SNOWFLAKE_ROLE=your-role

# Gong Configuration
GONG_API_KEY=your-gong-api-key
GONG_API_SECRET=your-gong-api-secret
GONG_ACCESS_KEY=your-gong-access-key

# Vercel Configuration
VERCEL_ACCESS_TOKEN=your-vercel-access-token
VERCEL_PROJECT_ID=your-project-id
VERCEL_ORG_ID=your-org-id
VERCEL_TEAM_ID=your-team-id

# Estuary Configuration
ESTUARY_API_KEY=your-estuary-api-key
ESTUARY_API_URL=https://api.estuary.dev

# GitHub Configuration (for secret management)
GITHUB_TOKEN=your-github-personal-access-token

# Pulumi Configuration (for secret management)
PULUMI_ACCESS_TOKEN=your-pulumi-access-token
PULUMI_ORGANIZATION=payready
PULUMI_PROJECT=sophia
PULUMI_STACK=development
